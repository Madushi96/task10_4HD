<%- include('header'); -%>

    <header>
        <div class="container-fluid bg-primary-subtle">
            <div class="col-sm-8 mx-auto text-center py-2">
                <h1 class="display-4">Creating and Awesome website</h1>
                <p class="lead ">Here are some quick guides on how you can implement awesome features to your website</p>
            </div>
        </div>
    </header>
<main>
    <div class="container mt-5">
        <h1 class="text-center">Users List</h1>
        <table class="table table-striped mt-4">
            <thead>
                <tr>
                    <th>Username</th>
                    <th>Hashed Password</th>
                </tr>
            </thead>
            <tbody>
                <% users.forEach(function(user) { %>
                    <tr>
                        <td><%= user.username %></td>
                        <td><%= user.password %></td>
                    </tr>
                <% }); %>
            </tbody>
        </table>
        <a href="/login" class="btn btn-primary mt-3">Back to Login</a>
    </div>

    <!-- Bootstrap JS -->
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0-alpha1/dist/js/bootstrap.bundle.min.js"></script>
</main>
</html>

